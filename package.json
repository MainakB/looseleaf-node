{
  "name": "looseleaf-node",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "start-dev": "concurrently \"npm run start-server\" \"npm run start-client\"",
    "build:browser": "node_modules/.bin/webpack",
    "prestart": "npm run build:browser",
    "start": "nodemon server/run-dev.js --watch server --watch client/shared --watch client/src",
    "build": "npm run build-client && npm run build-server",
    "build-server": "babel server/server.js -o server/build/main.bundle.js",
    "build-client": "cd  client && npm run build && cd ..",
    "start-server": "npm start",
    "start-server-prod": "forever start --minUptime 1000 --spinSleepTime 1000 -c babel-node server/server.js",
    "stop-server-prod": "forever stopall",
    "start-client": "node server/start-client.js",
    "dev": "echo \"This command has been deprecated. Use 'npm start'\" && exit 1",
    "lint:server": "./node_modules/.bin/eslint server --ext .js, .jsx --fix",
    "test": "mocha --compilers js:babel-core/register"
  },
  "dependencies": {
    "arraywrap": "^1.0.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.2",
    "cheerio": "^1.0.0-rc.2",
    "colors": "^1.1.2",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.3",
    "csurf": "^1.9.0",
    "dotenv": "^5.0.0",
    "ejs": "^2.5.7",
    "express": "^4.16.2",
    "express-enforces-ssl": "^1.1.0",
    "express-session": "^1.15.6",
    "forever": "^0.15.3",
    "fs": "0.0.1-security",
    "gravatar-url": "^2.0.0",
    "helmet": "^3.11.0",
    "mongoose": "^5.0.4",
    "morgan": "^1.9.0",
    "ms": "^2.1.1",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-github": "^1.1.0",
    "passport-google-oauth": "^1.0.0",
    "passport-local": "^1.0.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "source-map-support": "^0.5.3",
    "validator": "^9.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-es2015-parameters": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-app": "^3.1.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.9",
    "eslint": "^4.17.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.1",
    "eslint-plugin-react-native": "^3.2.1",
    "mocha": "^5.0.0",
    "nodemon": "^1.14.12",
    "style-loader": "^0.20.2",
    "supertest": "^3.0.0",
    "webpack": "^3.11.0"
  }
}
